{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/css/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.+\\.html)",
        "dest": "/My-First-Site/public/$1"
      },
      {
        "src": "/",
        "dest": "/My-First-Site/public/templates/home.html"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }